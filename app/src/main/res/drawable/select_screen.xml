<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:angle="90"
        android:centerColor="#8dcefc"
        android:endColor="#6cb2e2"
        android:startColor="#cdeaff"
        android:type="linear" />


    <stroke
        android:width="0dp"
        android:color="#ffffff"></stroke>

    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"></padding>

    <corners android:radius="0dp"></corners>
</shape>
